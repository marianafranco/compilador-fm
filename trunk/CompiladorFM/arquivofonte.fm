inteiro funcao(){
	inteiro param;
	inteiro var1;
	param = 0;
	var1 = 5;
	saida = var1;
	enquanto (param <= 2) {
		param = param + 1;
	}
	saida = param;
	retorno param;
}

principal () {

	inteiro var1;
	inteiro var2;
	
	
	var1 = 0;
	var2 = 3;
	
	funcao();
	
	enquanto (var1 < (var2+1) ){
		var1 = var1 + 1;
	}
	
	se (var1 == var2){
		saida = var1 + var2;
	
	}senao{
		var1 = var2;
		saida = var1;
	}
	
	var1 = (var1 * 5) / var1 + 3;
	saida = var1;
}